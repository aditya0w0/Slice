{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "prettier --write \"resources/**/*.blade.php\" \"resources/**/*.js\" \"resources/**/*.css\"",
        "prod": "vite build && php artisan config:cache && php artisan route:cache && php artisan view:cache",
        "prod:clean": "php artisan config:clear && php artisan route:clear && php artisan view:clear && npm run build && php artisan config:cache && php artisan route:cache && php artisan view:cache"
    },
    "devDependencies": {
        "@laravel/echo-react": "^2.2.6",
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-echo": "^2.2.6",
        "laravel-vite-plugin": "^2.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-blade": "^2.1.21",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "pusher-js": "^8.4.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7"
    },
    "dependencies": {
        "@radix-ui/react-avatar": "^1.1.11",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-separator": "^1.1.8",
        "@radix-ui/react-slot": "^1.2.4",
